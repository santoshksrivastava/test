.nav-tabs {border:none;}
.nav-tabs > li {    position:relative;margin:0 5px 0 0;    border: 1px solid #ccc;
    border-top-left-radius: 4px;    border-top-right-radius: 4px;border-bottom:none;}
.nav-tabs > li > a {    display:inline-block;padding:10px 20px 10px; color:#002462;font-weight:500;border-top-left-radius: 4px;    border-top-right-radius: 4px; }
.nav-tabs > li > span {    display:none;    cursor:pointer;    position:absolute;
    right: 5px;    top: 8px;    color: #fff;font-size:15px;font-weight:600;}
	.tb-cnt-border {border:#ccc solid 1px;}
.nav-tabs > li:hover > span {    display: inline-block;}

table caption {background:#002462;font-size:18px;padding:7px;}


.nav-tabs  li a.active , .nav-tabs  li a:hover {background:#002462;color:#fff;}
.nav-tabs  li a.active , .nav-tabs  li a:hover {color:#fff;}
.b-brand {font-size: 21px;    font-weight: 900;    color: #084298 !important;    text-transform: uppercase;line-height: 26px;}
.pc-sidebar .m-header {padding:15px!important;}
.pc-header .pc-head-link.active, .pc-header .pc-head-link:active, .pc-header .pc-head-link:focus, .pc-header .pc-head-link:hover {background:none!important;}

